<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.sample.AddProductController">
   <children>
      <Label layoutX="300.0" layoutY="71.0" text="Add Product">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TextField fx:id="titleField" layoutX="132.0" layoutY="182.0" prefHeight="42.0" prefWidth="230.0" promptText="title" />
      <TextArea fx:id="descriptionArea" layoutX="439.0" layoutY="182.0" prefHeight="200.0" prefWidth="230.0" promptText="description" />
      <TextField fx:id="stockField" layoutX="207.0" layoutY="237.0" prefHeight="26.0" prefWidth="78.0" promptText="1" text="1" />
      <TextField fx:id="priceField" layoutX="207.0" layoutY="287.0" prefHeight="26.0" prefWidth="78.0" promptText="0" text="0" />
      <Button fx:id="addButton" layoutX="361.0" layoutY="441.0" mnemonicParsing="false" onAction="#addProduct" prefHeight="26.0" prefWidth="77.0" text="Add">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="146.0" layoutY="237.0" text="stock:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="146.0" layoutY="287.0" text="price:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="300.0" layoutY="287.0" text="EUR">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
